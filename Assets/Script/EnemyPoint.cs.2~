using UnityEngine;
using System.Collections;

public class EnemyPoint : MonoBehaviour {

    /// 出現する敵指定
    public GameObject []enemyList;

    private float tmAll = 0;

	// Use this for initialization
	void Start () {
        
	}
  
  // Update is called once per frame
  void Update () {
    float oldTime = tmAll;
    tmAll += Time.deltaTime;
    if( oldTime <= 1.0 && 1.0 < tmAll ){
      GameObject.Instantiate( enemyList[0] );
    }
  }
  
}
  